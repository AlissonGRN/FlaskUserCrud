# Ignore the virtual environment directory
venv/

# Ignore Python cache files
__pycache__/
*.py[cod]

# Ignore SQLite database files (caso use um SQLite local para testes)
*.sqlite3

# Ignore environment variable files (usados para configurar credenciais, tokens, etc.)
.env
.config.py

# Ignore compiled libraries
*.so

# Ignore Jupyter Notebook checkpoints (caso use notebooks)
.ipynb_checkpoints/

# Ignore the migrations folder, as it's generated automatically
migrations/

# Ignore log files and database dumps
*.log
*.sql
*.sqlite

# Ignore system-specific files
.DS_Store
Thumbs.db

# Ignore any config file generated by your IDE or editor
.vscode/
.idea/
*.iml

# Ignore bytecode files
*.pyc

# Ignore any compressed or backup files
*.zip
*.tar.gz
*.bak
